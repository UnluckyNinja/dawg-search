{
  "name": "dawg-search",
  "type": "module",
  "version": "0.0.0",
  "description": "search multiple word in text, by leveraging both a DAWG and DAFSA",
  "author": "Author Name <author.name@mail.com>",
  "license": "MIT",
  "homepage": "https://github.com/author/library#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/author/library.git"
  },
  "bugs": {
    "url": "https://github.com/author/library/issues"
  },
  "exports": {
    ".": "./src/index.ts",
    "./*": "./*"
  },
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch --env.DEV=true",
    "dev:playground": "pnpm -r --include-workspace-root --parallel run dev",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "bumpp": "^10.4.1",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "fflate": "^0.8.2",
    "splaytree": "^3.2.3"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@^7.3.1"
    }
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.mjs",
      "./*": "./*"
    }
  }
}
